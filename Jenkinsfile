pipeline {
	agent any
	stages {
		stage('Init'){
			steps {
				echo 'Hi This is Kartik'
				echo 'We are starting testing'
			}
		}
		stage('Build'){
			steps {
				echo 'Building Simple Maven Project'
			}
		}
		stage('Deploy'){
			steps {
				echo 'Deploying Simple Maven Project'
			}
		}
		stage('Deploy Prod'){
			steps {
				echo 'Deploying Simple Maven Project to PROD'
			}
		}
	}
}